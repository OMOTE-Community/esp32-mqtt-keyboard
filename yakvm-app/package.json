{
  "name": "yakvm-app",
  "version": "1.0.0",
  "main": "main/backend.js",
  "type": "module",
  "scripts": {
    "start": "node dist/backend.js",
    "dev": "nodemon src/backend.ts",
    "build": "tsc",
    "generate": "protoc --plugin=./node_modules/.bin/protoc-gen-ts_proto --ts_out=src/generated --ts_opt=esModuleInterop=true,outputServices=none,outputEncodeMethods=true,outputJsonMethods=false --proto_path=../src ../src/keyevent.proto"
  },
  "dependencies": {
    "@bufbuild/protobuf": "^2.4.0",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "mqtt": "^5.0.0",
    "protobufjs": "^6.11.4"
  },
  "devDependencies": {
    "@types/cors": "^2.8.12",
    "@types/express": "^4.17.21",
    "@types/node": "^20.0.0",
    "nodemon": "^3.1.10",
    "ts-proto": "^2.7.0",
    "typescript": "^5.8.3"
  }
}
